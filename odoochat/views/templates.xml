<odoo>
    <data>
        <template id="chat_backend" name="Chat Assets Backend" inherit_id="web.assets_backend">
            <xpath expr="//script[@src='/web/static/src/js/report/client_action.js']" position="after">
                <div class="dn-chatter" style="display:none;">
                     <div id="online-users-list" class="hidemouseaway">
                        <ul class="list-group">
                        </ul>
                    </div>
                    <div class="chatbox" id="chatbox">
                        <span class="chat-text"></span>
                        <hr></hr>
                        <div class="smartchatbox">
                            <div class="messages" id="messages">
                                <ul id="chat_list"></ul>
                            </div>
                        </div>
                        <div class="close-chat" onclick="closeChatbox()"></div>
                        <div class="mini-chat" onclick="minimChatbox()">
                            <span class="minim-button">
                                <i class="icon fa fa-window-minimize"></i>
                            </span>
                        </div>
                        <div class="maxi-chat" onclick="maxiChatbox()" style="display:none">
                            <span class="maxi-button">
                                <i class="icon fa fa-plus"></i>
                            </span>
                        </div>
                        <div class="col-lg-12">
                            <form id="message-form" class="input-group">
                                <input autocomplete="off" type="text" class="form-control" placeholder="Type your message here" />
                                <span class="input-group-btn">
                                    <button type="submit" class="btn" id="send_btn">Send</button>
                                </span>
                            </form>
                        </div>
                    </div>
                    <script src="/odoochat/static/js/web.js"/>
                 </div>
            </xpath>
        </template>
    </data>
</odoo>