<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_op_class_form_inherit" model="ir.ui.view">
            <field name="name">op.class.form.inherit</field>
            <field name="model">op.batch</field>
            <field name="inherit_id" ref="openeducat_core.view_op_batch_form" />
            <field name="arch" type="xml">
                <field name="faculty_id" position="after">
                    <field name="room_id" context="{'terms': term_ids,'location':branch_id,'class_id': active_id,'days': days,'start': start,'end': end,'total_instr_hrs': total_instr_hrs}"/>

                </field>


            </field>
        </record>


    </data>
</odoo>
