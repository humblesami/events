<odoo>
    <data>
        <record id="action_foster_family_resorce" model="ir.actions.act_window">
            <field name="name">Foster Parents</field>
            <field name="res_model">foster.users</field>
            <field name="view_mode">kanban,tree,form</field>
            <field name="view_id" ref="foster_user_kanban"/>
            <!--[('groups_id.category_id.name','=','MeetingPoint'),'|',('groups_id.name','=','Admin'),('groups_id.name','=','Staff')]-->
            <field name="domain">
                [('groups_id.category_id.name','=','FosterManagement'),('groups_id.name','=','FamilyResource')]
            </field>
            <field name="context">{'dn_users_menu':True,'mp_family_resource_menu':True, 'user_type':'family_resource'}</field>

            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Click to create a user.
                </p>
            </field>
        </record>

        <record id="action_family_resource_kanban" model="ir.actions.act_window.view">
            <field name="act_window_id" ref="action_foster_family_resorce"/>
            <field name="sequence" eval="1"/>
            <field name="view_mode">kanban</field>
            <field name="view_id" ref="foster_user_kanban"/>
        </record>

        <record id="action_family_resource_form" model="ir.actions.act_window.view">
            <field name="act_window_id" ref="action_foster_family_resorce"/>
            <field name="sequence" eval="20"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="foster_management_user_form"/>
        </record>

        <menuitem
                id="menu_foster_family_resource"
                parent="menu_main_foster_users"
                name="Foster Parents"
                sequence="3"
                action="action_foster_family_resorce"
        />
    </data>
</odoo>