<odoo>
    <data>
        <record model="ir.ui.view" id="intake_face_sheet_tree">
            <field name="name">Client Intake Face Sheet tree</field>
            <field name="model">foster.intake</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="intake_worker"/>
                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="intake_face_sheet_form">
            <field name="name">Client Intake Face Sheet form</field>
            <field name="model">foster.intake</field>
            <field name="arch" type="xml">
                <form>
                    <notebook>
                        <page string="Youth Details">
                            <group>
                                <field name="youth_name_last"/>
                                <field name="youth_name_first"/>
                                <field name="pre_caretaker"/>
                                <field name="address"/>
                                <field name="phone" class="phone masked_input"/>
                                <field name="dob"/>
                                <field name="birth_place"/>
                                <field name="citizenship"/>
                                <field name="sex"/>
                                <field name="cultural_background"/>
                                <field name="racial_background"/>
                                <field name="religion"/>
                                <field name="lang_primary"/>
                                <field name="id_marks"/>
                                <field name="height"/>
                                <field name="weight"/>
                                <field name="hair_color"/>
                                <field name="eye_color"/>
                                <field name="ssn"/>
                                <field name="medicaid"/>
                                <field name="mass_no"/>
                                <field name="guardianship_status"/>
                                <field name="goal"/>
                                <field name="agency"/>
                                <field name="office"/>
                                <field name="agency_worker"/>
                            </group>
                            <h3>SUPERVISOR</h3>
                            <group>
                                <field name="supervisor"/>
                                <field name="supervisor_phone" class="phone masked_input"/>
                                <field name="supervisor_email"/>
                                <field name="supervisor_address"/>
                            </group>
                            <h3>CUSTODIAL AGENCY</h3>
                            <group>
                                <field name="custodial_agency"/>
                                <field name="other_agency"/>
                                <field name="agency_involvement_reasons" widget="many2many_tags"/>
                            </group>
                            <p>
                                Total Placements since Custodial Agencyâ€™s initial involvement:
                            </p>
                            <group>
                                <field name="foster_care"/>
                                <field name="therapeutic_foster_care"/>
                                <field name="residential"/>
                                <field name="hospitalization"/>
                                <field name="other"/>
                            </group>
                        </page>
                        <page string="Intake Worker">
                            <group>
                                <field name="intake_worker"/>
                                <field name="date"/>
                                <field name="pevious_intake"/>
                                <field name="foster_parent"/>
                                <field name="social_worker"/>
                                <field name="c_supervisor"/>
                                <field name="assign_date"/>
                            </group>
                        </page>
                        <page string="Parents">
                            <h3>BIRTH MOTHER</h3>
                            <group>
                                <field name="mother_married_name"/>
                                <field name="mother_maiden_name"/>
                                <field name="mother_first_name"/>
                                <field name="mother_address"/>
                                <field name="mother_phone" class="phone masked_input"/>
                                <field name="mother_background"/>
                                </group>
                            <h3>FATHER</h3>
                            <group>
                                <field name="father_last_name"/>
                                <field name="father_first_name"/>
                                <field name="father_address"/>
                                <field name="father_phone" class="phone masked_input"/>
                                <field name="father_background"/>
                            </group>
                        </page>
                        <page string="Emergency Contact Person">
                            <h3>EMERGENCY CONTACT PERSON</h3>
                            <field name="emergency_person"/>
                            <group>
                                <field name="family_contact"/>
                            </group>
                        </page>
                        <page string="Health Status">
                            <h3>PSYCHIATRIC DISORDERS</h3>
                            <group>
                                <field name="axis_1_primary"/>
                                <field name="axis_1_secondary"/>
                                <field name="axis_2"/>
                                <field name="axis_3"/>
                                <field name="axis_4"/>
                                <field name="axis_5"/>
                                <field name="high_risk_behave" widget="many2many_tags"/>
                                <field name="therapist"/>
                                <field name="clinic"/>
                                <field name="clinic_address"/>
                                <field name="clinic_phone"/>
                            </group>
                            <h3>FAMILT THERAPIST</h3>
                            <group>
                                <field name="family_therapist"/>
                                <field name="family_therapist_clinic"/>
                                <field name="family_therapist_phone" class="phone masked_input"/>
                                <field name="family_therapist_address"/>
                            </group>
                            <h3>FAMILY PHYCHIATRIST</h3>
                            <group>
                                <field name="family_psychiatrist"/>
                                <field name="family_psychiatrist_clinic"/>
                                <field name="family_psychiatrist_phone" class="phone masked_input"/>
                                <field name="family_psychiatrist_address"/>
                            </group>
                            <group>
                                <field name="last_physical_exam"/>
                                <field name="last_dental_exam"/>
                                <field name="medical_condition"/>
                                <field name="diet_type"/>
                                <field name="allergies"/>
                            </group>
                                <p style="font-size: 13px;font-weight: bold;">
                                    Current Psychotropic Medications/dosages/times of day, etc.
                                </p>
                                <field name="psychotropic"/>
                                <p style="font-size: 13px;font-weight: bold;">
                                    Current Non-psychotropic Medications/dosages/times of day, etc.
                                </p>
                                <field name="non_psychotropic"/>
                        </page>
                        <page string="Academics">
                            <group>
                                <field name="school"/>
                                <field name="grade"/>
                                <field name="IEP"/>
                                <field name="prob_officer"/>
                            </group>
                        </page>
                        <page string="Legal">
                            <h3>COURT</h3>
                            <group>
                                <field name="court"/>
                                <field name="court_phone" class="phone masked_input"/>
                            </group>
                            <h3>GAL</h3>
                            <group>
                                <field name="GAL"/>
                                <field name="GAL_phone" class="phone masked_input"/>
                            </group>
                            <h3>ATTORNEY</h3>
                            <group>
                                <field name="attorney"/>
                                <field name="attorney_phone" class="phone masked_input"/>
                            </group>
                            <group>
                                <field name="placement_restrictions" widget="many2many_tags"/>
                            </group>
                            <group>
                                <field name="respite_recommendations"/>
                                <field name="discharge_date"/>
                            </group>
                        </page>
                    </notebook>
                    <link href="/foster_base/static/resource_form.css" rel="stylesheet"/>
                </form>
            </field>
        </record>
        <record model="ir.actions.act_window" id="intake_face_sheet_action">
            <field name="name">Client Intake Face Sheet</field>
            <field name="res_model">foster.intake</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem
                id="client_menu"
                name="Client"
                sequence="3"
                parent="foster_management_menu_root"
        />

        <menuitem
                id="client_intake_face_sheet_menu"
                name="Client Intake Face Sheet"
                sequence="3"
                parent="client_menu"
                action="intake_face_sheet_action"
        />




        <!--emergency contact person tree-->
        <record model="ir.ui.view" id="emergency_youth_contact_person_tree">
            <field name="name">Emergency Contact Tree</field>
            <field name="model">youth.emergency.person</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="last_name"/>
                    <field name="first_name"/>
                </tree>
            </field>
         </record>

        <!--emergency contact person form-->
         <record model="ir.ui.view" id="emergency_youth_contact_person_form">
            <field name="name">Emergency Contact form</field>
            <field name="model">youth.emergency.person</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="last_name"/>
                        <field name="first_name"/>
                        <field name="phone" class="phone masked_input"/>
                        <field name="relation"/>
                        <field name="address"/>
                    </group>
                </form>
            </field>
         </record>
    </data>
</odoo>