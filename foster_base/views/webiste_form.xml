<odoo>
    <data>
        <template id="foster_application_page" name="Foster Application Page">
            <t t-call="website.layout">
                <style>

                    .wrap {
                    padding: 30px;
                    }
                    .wrap .control-label {
                    text-align: left;
                    }
                </style>
                <div class="wrap">
                    <h3>Applicant Details</h3>
                    <form action="/application/submit" method="POST" class="form-horizontal mt32" enctype="multipart/form-data">
                        <div class=" form-group"><label class="col-md-2 col-sm-3 control-label"> </label><label class="col-md-2 dn_error" style="color:red; width:620px"></label></div>
                        <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>


                        <!--Date field-->
                        <div t-attf-class="form-group #{error and 'date' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="date">Date</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="date" class="form-control" name="date" required="True" t-attf-value="#{date or ''}"/>
                            </div>
                        </div>
                        <!--Char field-->
                        <div t-attf-class="form-group #{error and 'last_name' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="last_name">Last Name</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="last_name" required="True" t-attf-value="#{last_name or ''}"/>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'last_name' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="first_name">First</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="first_name" required="True" t-attf-value="#{first_name or ''}"/>
                            </div>
                        </div>
                        <div t-attf-class="form-group #{error and 'middle_name' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="middle_name">Middle</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="middle_name" t-attf-value="#{middle_name or ''}"/>
                            </div>
                        </div>
                        <div t-attf-class="form-group #{error and 'other_name' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="other_name">Any Other Name</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="other_name" t-attf-value="#{other_name or ''}"/>
                            </div>
                        </div>
                        <!--selection field-->
                        <div t-attf-class="form-group #{error and 'sex' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="sex">Sex</label>
                            <div class="col-md-10 col-sm-9">
                                <select type="text" name="sex" class="form-control">
                                    <option></option>
                                    <option value="male">Male</option>
                                    <option value="female">Female</option>
                                    <option value="other">Other</option>
                                </select>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'birth_date' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="birth_date">Birth Date</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="date" class="form-control" name="birth_date" required="True" t-attf-value="#{birth_date or ''}"/>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'place_of_birth' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="place_of_birth">Place of Birth</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="place_of_birth" t-attf-value="#{place_of_birth or ''}"/>
                            </div>
                        </div>
                        <div t-attf-class="form-group #{error and 'email' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="email">Email</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="email" class="form-control" name="email" t-attf-value="#{email or ''}"/>
                            </div>
                        </div>
                        <h4 style="margin-left: -6px;font-weight: bold;">ADDRESS</h4>
                        <div t-attf-class="form-group #{error and 'address' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="address">Address</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="address" t-attf-value="#{address or ''}"/>
                            </div>
                        </div>
                        <div t-attf-class="form-group #{error and 'town' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="town">Town</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="town" t-attf-value="#{town or ''}"/>
                            </div>
                        </div>
                        <!--many2one field-->
                        <div t-attf-class="form-group #{error and 'state' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="state">State</label>
                            <div class="col-md-10 col-sm-9">
                                <select class="form-control" id="state" name="states">
                                    <option></option>
                                    <t t-foreach="states" t-as="state">
                                        <option t-attf-value="#{state.id}"><t t-esc="state.name"/></option>
                                    </t>
                                </select>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'zip' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="zip">Zip</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="zip"/>
                            </div>
                        </div>
                        <h4 style="margin-left: -6px;font-weight: bold;">TELEPHONE</h4>
                        <div t-attf-class="form-group #{error and 'home_phone' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="home_phone">Home Phone</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="home_phone" t-attf-value="#{home_phone or ''}"/>
                            </div>
                        </div>
                        <div t-attf-class="form-group #{error and 'work_phone' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="work_phone">Work Phone</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="work_phone" t-attf-value="#{work_phone or ''}"/>
                            </div>
                        </div>
                        <div t-attf-class="form-group #{error and 'cell_phone' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="cell_phone">Cell/Mobile Phone</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="cell_phone" t-attf-value="#{cell_phone or ''}"/>
                            </div>
                        </div>
                        <div t-attf-class="form-group #{error and 'security_number' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="security_number">Social Security Number</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="security_number" t-attf-value="#{security_number or ''}"/>
                            </div>
                        </div>
                        <h4 style="margin-left: -6px;font-weight: bold;">CITIZENSHIP</h4>
                        <div t-attf-class="form-group #{error and 'country' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="country">Country</label>
                            <div class="col-md-10 col-sm-9">
                                <select class="form-control" id="country" name="countries">
                                    <option></option>
                                    <t t-foreach="countries" t-as="country">
                                        <option t-attf-value="#{country.id}"><t t-esc="country.name"/></option>
                                    </t>
                                </select>
                            </div>
                        </div>
                        <div t-attf-class="form-group #{error and 'immigration' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="immigration">If not Citizen,Immigration Status</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="immigration" t-attf-value="#{immigration or ''}"/>
                            </div>
                        </div>
                        <!--Languages-->
                        <h4 style="margin-left: -6px;font-weight: bold;">EDUCATION</h4>
                        <div t-attf-class="form-group #{error and 'education' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="education">Education</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="education" t-attf-value="#{education or ''}"/>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'last_grade' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="last_grade">Last Grade</label>
                            <div class="col-md-10 col-sm-9">
                                <select type="text" name="last_grade" class="form-control">
                                    <option></option>
                                    <option value="a">A</option>
                                    <option value="b">B</option>
                                    <option value="c">C</option>
                                    <option value="d">D</option>
                                </select>
                            </div>
                        </div>
                        <h4 style="margin-left: -6px;font-weight: bold;">LANGUAGES</h4>
                        <h5 style="margin-left: -6px">Spoken in household</h5>
                        <div t-attf-class="form-group #{error and 'lang_primary' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="lang_primary">Primary</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="lang_primary" t-attf-value="#{lang_primary or ''}"/>
                            </div>
                        </div>
                        <div t-attf-class="form-group #{error and 'lang_other' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="lang_other">Other</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="lang_other" t-attf-value="#{lang_other or ''}"/>
                            </div>
                        </div>
                        <h5 style="margin-left: -6px">Written Comprehension</h5>
                        <div t-attf-class="form-group #{error and 'comp_primary' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="comp_primary">Primary</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="comp_primary" t-attf-value="#{comp_primary or ''}"/>
                            </div>
                        </div>
                        <div t-attf-class="form-group #{error and 'comp_other' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="comp_other">Other</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="comp_other" t-attf-value="#{comp_other or ''}"/>
                            </div>
                        </div>
                        <h4 style="margin-left: -6px;font-weight: bold;">NOTES</h4>
                        <h5 style="margin-left: -6px;">Direction to your home</h5>
                        <div t-attf-class="form-group #{error and 'note' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="note">Notes</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="note" t-attf-value="#{note or ''}"/>
                            </div>
                        </div>
                        <h4 style="margin-left: -6px;font-weight: bold;">MARRIAGE</h4>
                        <div t-attf-class="form-group #{error and 'current_marriage_date' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="current_marriage_date">Current Marriage Date</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="date" class="form-control" name="current_marriage_date" t-attf-value="#{current_marriage_date or ''}"/>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'previous_marriage_date' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="previous_marriage_date">Previous Marriage Date</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="date" class="form-control" name="previous_marriage_date" t-attf-value="#{previous_marriage_date or ''}"/>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'previous_marriage_date_end' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="previous_marriage_date_end">Previous Marriage Date Ended</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="date" class="form-control" name="previous_marriage_date_end" t-attf-value="#{previous_marriage_date_end or ''}"/>
                            </div>
                        </div>
                        <!--Employment Details-->
                        <div t-attf-class="form-group #{error and 'employment_type' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="employment_type">Last Grade</label>
                            <div class="col-md-10 col-sm-9">
                                <select type="text" name="employment_type" class="form-control">
                                    <option></option>
                                    <option value="employee">Employee</option>
                                    <option value="worker">Worker</option>
                                    <option value="self-employed">Self-Employed</option>
                                </select>
                            </div>
                        </div>
                        <div t-attf-class="form-group #{error and 'days_worked' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="days_worked">Hours/Days Worked</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="days_worked" t-attf-value="#{days_worked or ''}"/>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'date_employment_began' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="date_employment_began">Date Employment Began</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="date" class="form-control" name="date_employment_began" t-attf-value="#{date_employment_began or ''}"/>
                            </div>
                        </div>

                         <div t-attf-class="form-group #{error and 'income_year' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="income_year">Income Per Year</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="income_year" t-attf-value="#{income_year or ''}"/>
                            </div>
                        </div>

                        <h4 style="margin-left: -6px;font-weight: bold;">EMPLOYER</h4>
                        <div t-attf-class="form-group #{error and 'employer_name' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="employer_name">Employer Name</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="employer_name" t-attf-value="#{employer_name or ''}"/>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'employer_phone' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="employer_phone">Employe Phone</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="employer_phone" t-attf-value="#{employer_phone or ''}"/>
                            </div>
                        </div>
                        <h4 style="margin-left: -6px;font-weight: bold;">HOUSING</h4>
                        <div t-attf-class="form-group #{error and 'house' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="house">House</label>
                            <div class="col-md-10 col-sm-9">
                                <select type="text" name="house" class="form-control">
                                    <option></option>
                                    <option value="yes">Own</option>
                                    <option value="no">Rent</option>
                                </select>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'current_address_period' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="current_address_period">How long at current address</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="current_address_period" t-attf-value="#{current_address_period or ''}"/>
                            </div>
                        </div>
                        <div t-attf-class="form-group #{error and 'name_of_contact' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="name_of_contact">Name of contact for verification</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="name_of_contact" t-attf-value="#{name_of_contact or ''}"/>
                            </div>
                        </div>
                        <div t-attf-class="form-group #{error and 'phone_of_contact' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="phone_of_contact">Phone Number of contact for verification</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="phone_of_contact" t-attf-value="#{phone_of_contact or ''}"/>
                            </div>
                        </div>
                        <div t-attf-class="form-group #{error and 'previous_address' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="previous_address">Previous Address and how long</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="previous_address" t-attf-value="#{previous_address or ''}"/>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-md-offset-2 col-sm-offset-3 col-sm-9 col-md-10">
                                <button class="btn btn-primary btn-lg">Submit</button>
                            </div>
                        </div>
                    </form>

                </div>
            </t>
        </template>

        <!--Partner Form-->
        <template id="foster_partner_page" name="Foster Application Page">
            <t t-call="website.layout">
                <style>

                    .wrap {
                    padding: 30px;
                    }
                    .wrap .control-label {
                    text-align: left;
                    }
                </style>
                <div class="wrap">
                    <h3>Partner Details</h3>
                    <form action="/partner/submit" method="POST" class="form-horizontal mt32" enctype="multipart/form-data">
                        <div class=" form-group"><label class="col-md-2 col-sm-3 control-label"> </label><label class="col-md-2 dn_error" style="color:red; width:620px"></label></div>
                        <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>

                        <div t-attf-class="form-group #{error and 'spouse_last_name' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="spouse_last_name">Last Name</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="spouse_last_name" required="True" t-attf-value="#{spouse_last_name or ''}"/>
                            </div>
                        </div>
                        <div t-attf-class="form-group #{error and 'spouse_first_name' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="spouse_first_name">First Name</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="spouse_first_name" required="True" t-attf-value="#{spouse_first_name or ''}"/>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'spouse_middle_name' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="spouse_middle_name">Middle</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="spouse_middle_name"  t-attf-value="#{spouse_middle_name or ''}"/>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'spouse_other_name' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="spouse_other_name">Any Other Name</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="spouse_other_name"  t-attf-value="#{spouse_other_name or ''}"/>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'age' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="age">Age</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="age" t-attf-value="#{age or ''}"/>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'co_home_phone' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="co_home_phone">Home Phone</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="co_home_phone"  t-attf-value="#{co_home_phone or ''}"/>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'co_cell_phone' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="co_cell_phone">Cell/Mobile Phone</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="co_cell_phone" t-attf-value="#{co_cell_phone or ''}"/>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'co_work_phone' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="co_work_phone">Work Phone</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="co_work_phone"  t-attf-value="#{co_work_phone or ''}"/>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'security_number' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="security_number">Social Security Number</label>
                            <div class="col-md-10 col-sm-9">
                                <input type="text" class="form-control" name="security_number"  t-attf-value="#{security_number or ''}"/>
                            </div>
                        </div>

                        <div t-attf-class="form-group #{error and 'co_applicant_email' in error and 'has-error' or ''}">
                            <label class="col-md-2 col-sm-3 control-label" for="co_applicant_email">Email</label>
                            <div class="col-md-10 col-sm-9">
