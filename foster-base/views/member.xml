<odoo>
    <data>
        <record model="ir.ui.view" id="foster_family_members_list">
            <field name="name">foster_family_members list</field>
            <field name="model">foster.family.members</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="full_name"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="foster_family_members_form">
            <field name="name">foster_family_members form</field>
            <field name="model">foster.family.members</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="full_name"/>
                        <field name="sex"/>
                        <field name="birth_date"/>
                        <field name="security_number" class="phone masked_input"/>
                        <field name="living_at_hom"/>
                        <field name="relation_to_applicant"/>
                    </group>
                     <h4>
                        MEDICAL Reference(s)-One required for each household member
                    </h4>
                    <field name= "medical_reference"/>

                </form>
            </field>
        </record>

        <!--other living members in the home-->
        <record model="ir.ui.view" id="foster_other_members_list">
            <field name="name">foster_other_members list</field>
            <field name="model">foster.other.members</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="full_name"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="foster_other_members_form">
            <field name="name">foster_other_members form</field>
            <field name="model">foster.other.members</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="full_name"/>
                        <field name="sex"/>
                        <field name="birth_date"/>
                        <field name="security_number" class="phone masked_input"/>
                        <field name="living_at_home"/>
                        <field name="nature"/>
                    </group>
                     <h4>
                        MEDICAL Reference(s)-One required for each household member
                    </h4>
                    <field name= "medical_reference"/>

                </form>
            </field>
        </record>
        <!--Foster history-->
        <record model="ir.ui.view" id="foster_history_form">
            <field name="name">foster_history form</field>
            <field name="model">foster.history</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name"/>
                        <field name="date"/>
                    </group>

                </form>
            </field>
        </record>
        <!--Providing foster-->
        <record model="ir.ui.view" id="foster_providing_form">
            <field name="name">providing.foster form</field>
            <field name="model">providing.foster</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name"/>
                        <field name="agency"/>
                    </group>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="foster_family_childcare_form">
            <field name="name">family.childcare form</field>
            <field name="model">family.childcare</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name"/>
                    </group>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="foster_disabled_childcare_form">
            <field name="name">disabled.child.care form</field>
            <field name="model">disable.childcare</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name"/>
                        <field name="care_taker"/>
                        <field name="reason"/>
                        <field name="agency"/>
                    </group>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="foster_childcare_plan_person_form">
            <field name="name">childcare.plan_person form</field>
            <field name="model">childcare.plan.person</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name"/>
                    </group>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="foster_childcare_plan_services_form">
            <field name="name">childcare.plan_services form</field>
            <field name="model">childcare.plan.services</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name"/>
                        <field name="description"/>
                    </group>
                </form>
            </field>
        </record>
    </data>
</odoo>