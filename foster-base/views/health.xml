<odoo>
    <data>
        <record model="ir.ui.view" id="foster_health_histor_list">
      <field name="name">foster_health_history list</field>
      <field name="model">health.history</field>
      <field name="arch" type="xml">
        <tree>
          <field name="member_name"/>
        </tree>
      </field>
    </record>
        <record model="ir.ui.view" id="foster_health_history_form">
            <field name="name">foster_health_history form</field>
            <field name="model">health.history</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="member_name"/>
                        <field name="provider_name"/>
                        <field name="address"/>
                        <field name="phone" class="phone masked_input"/>
                        <field name="treat_type"/>
                    </group>
                </form>
            </field>
        </record>

           <record model="ir.ui.view" id="foster_medical_problems_list">
      <field name="name">foster_medical_problems list</field>
      <field name="model">medical.problems</field>
      <field name="arch" type="xml">
        <tree>
          <field name="member_name"/>
        </tree>
      </field>
    </record>
        <record model="ir.ui.view" id="foster_medical_problems_form">
            <field name="name">foster_medical_problems form</field>
            <field name="model">medical.problems</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="member_name"/>
                        <field name="provider_name"/>
                        <field name="address"/>
                        <field name="phone" class="phone masked_input"/>
                        <field name="date"/>
                    </group>
                </form>
            </field>
        </record>
    </data>
</odoo>