<odoo>
    <data>
        <record id="action_foster_manger" model="ir.actions.act_window">
            <field name="name">Managers</field>
            <field name="res_model">foster.users</field>
            <field name="view_mode">kanban,tree,form</field>
            <field name="view_id" ref="foster_user_kanban"/>
            <!--[('groups_id.category_id.name','=','MeetingPoint'),'|',('groups_id.name','=','Admin'),('groups_id.name','=','Staff')]-->
            <field name="domain">
                [('groups_id.category_id.name','=','FosterManagement'),('groups_id.name','=','Manager')]
            </field>
            <field name="context">{'dn_users_menu':True,'mp_manager_menu':True, 'user_type':'manager'}</field>

            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Click to create a user.
                </p>
            </field>
        </record>

        <record id="action_manger_kanban" model="ir.actions.act_window.view">
            <field name="act_window_id" ref="action_foster_manger"/>
            <field name="sequence" eval="1"/>
            <field name="view_mode">kanban</field>
            <field name="view_id" ref="foster_user_kanban"/>
        </record>

        <record id="action_manger_form" model="ir.actions.act_window.view">
            <field name="act_window_id" ref="action_foster_manger"/>
            <field name="sequence" eval="20"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="foster_management_user_form"/>
        </record>

        <menuitem
                id="menu_foster_manger"
                parent="menu_foster_users"
                name="Managers"
                sequence="4"
                action="action_foster_manger"
        />
    </data>
</odoo>