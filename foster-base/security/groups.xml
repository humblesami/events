<odoo>
    <data>
        <record id="module_category_foster" model="ir.module.category">
            <field name="name">FosterManagement</field>
            <field name="description">FosterManagement</field>
            <field name="sequence">20</field>
        </record>

        <record model="res.groups" id="group_foster_base">
            <field name="name">FosterManagement</field>
        </record>

         <record model="res.groups" id="group_case_worker">
            <field name="name">CaseWorker</field>
            <field name="category_id" ref="module_category_foster"/>
            <field name="implied_ids" eval="[(4, ref('group_foster_base')),
            (4, ref('base.group_user'))]"/>
            <field name="user_model_ids" eval="[(4, ref('foster-base.model_foster_users'))]"/>
        </record>

        <record model="res.groups" id="group_foster_supervisor">
            <field name="name">Supervisor</field>
            <field name="category_id" ref="module_category_foster"/>
            <field name="implied_ids" eval="[(4, ref('group_foster_base')),
            (4, ref('base.group_user'))]"/>
            <field name="user_model_ids" eval="[(4, ref('foster-base.model_foster_users'))]"/>
        </record>
        <record model="res.groups" id="group_foster_manager">
            <field name="name">Manager</field>
            <field name="category_id" ref="module_category_foster"/>
            <field name="implied_ids" eval="[(4, ref('group_foster_base')),
            (4, ref('base.group_user'))]"/>
            <!--<field name="users" eval="[(4, ref('base.user_root'))]"/>-->
            <field name="user_model_ids" eval="[(4, ref('foster-base.model_foster_users'))]"/>
        </record>
         <record model="res.groups" id="group_foster_admin">
            <field name="name">Admin</field>
            <field name="category_id" ref="module_category_foster"/>
             <field name="implied_ids" eval="[(4, ref('group_foster_base')),
            (4, ref('base.group_user'))]"/>
            <field name="user_model_ids" eval="[(4, ref('foster-base.model_foster_users'))]"/>
        </record>
         <record model="res.groups" id="group_foster_family_resource">
            <field name="name">FamilyResource</field>
            <field name="category_id" ref="module_category_foster"/>
             <field name="implied_ids" eval="[(4, ref('group_foster_base')),
            (4, ref('base.group_user'))]"/>
             <field name="user_model_ids" eval="[(4, ref('foster-base.model_foster_users'))]"/>

        </record>
    </data>
</odoo>