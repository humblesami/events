<odoo>
    <data>
        <!--APPROVAL RULES -->
        <record id="record_leave_rules_action" model="ir.actions.act_window">
            <field name="name">Approval Rules</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">custom_leave_management.approval_rules</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>


        <record id="record_leave_rule_model_action" model="ir.actions.act_window">
            <field name="name">Rules Allocation</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">custom_leave_management.rule_authorization</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>

        <!--APPROVAL RULE VIEWS -->

        <record model="ir.ui.view" id="custom_leave_approval_rule_tree_view">
            <field name="name">custom_leave.approval.rule.tree</field>
            <field name="model">custom_leave_management.approval_rules</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="custom_leave_approval_rule_form_view" model="ir.ui.view">
            <field name="name">custom_leave.approval.rule.form</field>
            <field name="model">custom_leave_management.approval_rules</field>
            <field name="priority" eval="20"/>
            <field name="arch" type="xml">
                <form string="Approval Rule">
                    <sheet>
                        <group>
                            <field name="name" class="oe_inline"/>

                            <field name="approver_ids">
                                <tree editable="bottom">
                                    <field name="group_id"></field>
                                    <field name="sequence"></field>
                                </tree>
                                <form>
                                    <sheet>
                                        <group>
                                            <field name="group_id"></field>
                                            <field name="sequence"></field>
                                        </group>
                                    </sheet>
                                </form>
                            </field>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <!--ENDING APPROVAL Rules -->

        <!--RULE ATHURIZATION -->
        <record id="custom_leave_rule_authorization_tree_view" model="ir.ui.view">
            <field name="name">custom_leave.rule_authorization.tree.view</field>
            <field name="model">custom_leave_management.rule_authorization</field>
            <field name="priority" eval="20"/>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="model_id"></field>
                    <field name="rule_id"></field>
                    <field name="group_id"></field>
                </tree>
            </field>
        </record>
        <record id="custom_leave_rule_authorization_form_view" model="ir.ui.view">
            <field name="name">custom_leave.rule_authorization.form.view</field>
            <field name="model">custom_leave_management.rule_authorization</field>
            <field name="priority" eval="20"/>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="model_id" domain="[()]" options="{'no_create_edit': True}"></field>
                            <field name="rule_id" options="{'no_create_edit': True}"></field>
                            <field name="group_id" domain="[()]" options="{'no_create_edit': True}"></field>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <!--ENDING RULE AUTHORIZATION -->


        <!--HR HOLIDAYS INHERITED -->

        <record id="hr_holidays_form_inherited" model="ir.ui.view">
            <field name="name">hr.holidays.form.inherited</field>
            <field name="model">hr.holidays</field>
            <field name="inherit_id" ref="hr_holidays.edit_holiday_new"/>
            <field name="arch" type="xml">

                <group position="after">
                    <notebook>
                        <page string="Approbations">
                            <field name="approbation">
                                <tree create="false" edit="false" readonly="1">
                                    <field name="leave"></field>
                                    <field name="approver"></field>
                                    <field name="date"></field>
                                    <field name="state"></field>
                                    <field name="reason"/>

                                </tree>
                            </field>
                        </page>
                    </notebook>
                </group>
                <data>
                    <xpath expr="//field[@name='payslip_status']" position="after">
                        <field name="pending_approver"></field>
                    </xpath>
                </data>
                <header position="before">
                    <field name="current_user_is_approver" invisible="1"></field>
                </header>
                <header position="attributes">
                    <attribute name="attrs">{'invisible':[('current_user_is_approver', '=', False)]}</attribute>
                </header>
                <button name="action_approve" position="attributes">
                    <attribute name="groups"></attribute>
                </button>
                <button name="action_validate" position="attributes">
                    <attribute name="groups"></attribute>
                </button>
                <button name="action_refuse" position="attributes">
                    <attribute name="groups"></attribute>
                </button>
                <field name="department_id" position="replace">
                    <field name="department_id"
                           attrs="{'readonly':['|','|' ,('type','=','add'),('holiday_type','=','category'),('state','=', 'validate')],'invisible':[('holiday_type','=','category')]}"/>
                </field>
                <field name="holiday_type" position="replace">
                    <field name="holiday_type" attrs="{'readonly':[('type', '=', 'remove'),('state','!=','draft')]}"
                           string="Mode" context="{'employee_id':employee_id}"/>
                </field>
                <field name="employee_id" position="replace">
                    <field name="employee_id"
                           attrs="{'required':[('holiday_type','=','employee')],'invisible':[('holiday_type','=','category')]}"/>
                </field>
                <field name="employee_id" position="attributes">
                    <attribute name="attrs">{'readonly':[('current_user_is_approver', '=', False)]}</attribute>
                </field>
                <field name="department_id" position="attributes">
                    <attribute name="attrs">{'readonly':[('current_user_is_approver', '=', False)]}</attribute>
                </field>

            </field>
        </record>


        <!--RES GROUP FORM VIEW DUPLICATE -->

        <record id="custom_leave_view_groups_tree" model="ir.ui.view">
            <field name="name">custom.leave.res.groups.tree</field>
            <field name="model">res.groups</field>
            <field name="mode">primary</field>
            <field eval="27" name="priority"/>
            <field name="arch" type="xml">
                <tree string="Access Groups">
                    <field name="full_name" modifiers="{'readonly': true}"/>
                </tree>
            </field>
        </record>


        <record id="custom_leave_view_groups_form" model="ir.ui.view">
            <field name="name">custom.leave.res.groups.form</field>
            <field name="model">res.groups</field>
            <field name="mode">primary</field>
            <field eval="25" name="priority"/>
            <field name="arch" type="xml">
                <form string="Groups">
                    <sheet>
                        <group>
                            <field name="name"
                                   attrs="{'readonly':['|',('name', '=', 'Admin'),('name', '=', 'Staff')]}"/>
                        </group>
                        <notebook>
                            <page string="Users">
                                <field name="users" context="{'search_default_no_share':1}"/>
                            </page>
                            <page string="Access Rights">
                                <field name="model_access">
                                    <tree string="Access Rules" editable="bottom" create="false" delete="false">
                                        <field name="model_id"/>
                                        <field name="perm_read"/>
                                        <field name="perm_write"/>
                                        <field name="perm_create"/>
                                        <field name="perm_unlink"/>
                                        <field name="name"/>
                                    </tree>
                                    <form string="Access Controls">
                                        <group col="4">
                                            <field name="name"/>
                                            <field name="active"/>
                                            <field name="model_id"/>
                                            <newline/>
                                            <field name="perm_read"/>
                                            <field name="perm_write"/>
                                            <field name="perm_create"/>
                                            <field name="perm_unlink"/>
                                        </group>
                                    </form>
                                </field>
                            </page>
                            <page string="Notes">
                                <field name="comment"/>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>


