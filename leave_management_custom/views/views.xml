                <h2><field name="login"/></h2>
            </div>
                <group string="Status" name="active_group">
                        <field name="groups_id" widget="many2many_checkboxes" domain="[('category_id.name','=','E-Leave')]" required="1"/>
                </group>
            <group>
                    <group string="Contact Information">
                         <field name="address_id" context="{'show_address': 1}" options="{&quot;always_reload&quot;: True, &quot;highlight_first_line&quot;: True}"/>
                        <field name="work_phone"/>
                        <field name="mobile_phone" widget="email"/>
                        <field name="work_location"/>
                        <field name="job_id"/>
                        <field name="department_id"/>
                        <field name="bio"/>
                        <field name="resume"/>
                    </group>

                </group>
        </sheet>
        <script src="/leave_management_custom/static/js/profile.js">

        </script>
    </form>

            </field>
