<odoo>
    <data>
        <record id="view_meeting_dashboard" model="ir.ui.view">
            <field name="name">meeting_point.dashboard</field>
            <field name="model">meeting_point.dashboard</field>
            <field name="arch" type="xml">
                <form>
                    <link href="/dn_auth/static/css/login-info.css" rel="stylesheet"/>
                    <div class="wrapper">
                        <div class="main-panel">
                            <div class="content">
                                <div class="OdooDashboardEditTableWrapper">
                                <div class="user-conatiner">
                                    <h3>Specify Users</h3>
                                    <div class="OdooDashboardTableWrapper">
                                    <select id="user-drop-down" multiple="multiple">
                                        <option value="1">One</option>
                                        <option value="2">TOne</option>
                                        <option value="3">ThOne</option>
                                    </select>
                                    <!--<field name="partner_ids" widget="many2manyattendee" options="{'no_quick_create': True, 'no_create_edit' : True}" placeholder="Select Users..." class="oe_inline"/>-->
                                    <span class="form-item">
                                        Gruop By User : <input type="checkbox" id="grouped-by-user" checked="checked"/>
                                    </span>
                                    <span id="unique-user-checkbox" class="form-item">
                                        Just Last Login: <input type="checkbox" id="unique-user" />
                                    </span>
                                </div>
                                </div>
                                <div class="form-bottom">
                                    <span class="form-item">
                                        Interval Type:
                                        <select id="interval-type">
                                            <option value="Duration">Duration</option>
                                            <option value="Date">Date</option>
                                        </select>
                                    </span>

                                    <span class="form-item" id="date-div">
                                        <span class="inner-items">
                                            From: <input type="date" name="date_from" id="date-from" placeholder="End Date"/>
                                        </span>
                                        <span class="inner-items">
                                            To: <input type="date" name="date_to" id="date-to" placeholder="Start Date"/>
                                        </span>
                                    </span>

                                    <span class="form-item" id="duration-div">
                                        <span class="inner-items">
                                            Duration Type:
                                            <select id="duration-type">
                                                <option value="Days">Days</option>
                                                <option value="Hours">Hours</option>
                                                <option value="Months">Months</option>
                                            </select>
                                        </span>
                                        <span class="inner-items">
                                            Set Duration: <input type="number" value="7" name="duration" id="duration" placeholder="Select between 1-10000"/>
                                        </span>
                                    </span>
                                    <span class="submit btn-primary" onclick="fetch_logins()">Get Records</span>
                                </div>
                                </div>
                                <div id="ungrouped-logins"/>
                                <div id="grouped-logins"/>
                                <div id="prev-logins"/>
                            </div>
                        </div>
                    </div>
                    <script src="/dn_auth/static/js/login-info.js"/>
                </form>
            </field>
        </record>

        <record id="action_admin_dashboard" model="ir.actions.act_window">
            <field name="name">Dashboard</field>
            <field name="res_model">meeting_point.dashboard</field>
            <field name="view_id" ref="view_meeting_dashboard" />
        </record>

    </data>
</odoo>