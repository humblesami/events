<odoo>
    <data>

        <template id="assets_backend" inherit_id="web.assets_backend" name="Web Custom Assets" >
            <xpath expr=".">
                <script type="application/javascript" src="/dn_auth/static/js/web.js"/>
            </xpath>
        </template>

        <record id="login_info_tree" model="ir.ui.view">
            <field name="name">login.info.tree</field>
            <field name="model">login.info</field>
            <field name="arch" type="xml">
                <tree string="Login Info" default_order="create_date desc">
                    <field name="user_id"/>
                    <field name="email" string="Email"/>
                    <field name="create_date" string="Login Time"/>
                    <field name="ip" string="IP Address"/>
                    <field name="location"/>
                    <field name="platform"/>
                    <field name="browser"/>
                </tree>
            </field>
        </record>

        <record id="action_login_info" model="ir.actions.act_window">
            <field name="name">Login Info</field>
            <field name="res_model">login.info</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem
                id="menu_login_info"
                name="Login Info"
                parent="dn_base.dev_menu"
                action="action_login_info"
                groups="base.group_system"
        />

    </data>
</odoo>